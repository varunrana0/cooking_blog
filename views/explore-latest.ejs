<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">Latest Recipes</li>
  </ol>
</nav>

<div class="explore_latest row row-cols-2 row-cols-lg-3 g-3 g-lg-4 mt-3">
  <% if(recipes !== ''){%> <% recipes.forEach(function(recipe, index){ %>

  <a href="/recipe/<%- recipe._id %> " class="col category__link text-center">
    <div class="category__img--large shadow">
      <img src="/uploads/<%- recipe.image%>" alt="<%- recipe.name%>" />
    </div>
    <div class="pt-1"><%- recipe.name%></div>
  </a>

  <% })} %>
</div>
